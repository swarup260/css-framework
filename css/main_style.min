*{box-sizing:border-box;margin:0}.top-navbar ul{list-style:none;background-color:#696368;width:100%}.top-navbar li{display:inline-block;text-align:center;margin:2px 0;padding:5px}.top-navbar a{color:#fff;text-decoration:none}.top-navbar a:hover{background-color:#696399;font-size:30px}.Topfixed ul{overflow:hidden;top:0;position:fixed}p{font-size:1em}.border-style li{border-right:2px #000 solid}.border-style li:last-child{border:none}.active{background-color:#4CC23F}@media only screen and (max-width:760px){.top-navbar li{display:block}.border-style li{border-bottom:2px #000 solid}.border-style li:last-child{border:none}}.side-navbar ul{margin:0;padding:0;background-color:#939292;list-style:none;width:25%}.side-navbar li a{text-decoration:none;display:block;text-align:center;padding:8px 16px}.side-navbar a:hover{background-color:#696399;font-size:30px}.SideFixed ul{position:fixed;overflow:auto;height:100%}.Sideborder ul{border:1px solid #000}.Sideborder li{border-bottom:1px #000 solid}.Sideborder li:last-child{border:none}